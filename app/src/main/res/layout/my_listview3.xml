<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <TextView
        android:id="@+id/textSymbol"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"

        android:text="TextView"
        android:background="#FFF"
        android:textColor="#000"
        tools:layout_editor_absoluteX="38dp"
        tools:layout_editor_absoluteY="11dp"

        />

    <TextView
        android:id="@+id/textPrice"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_alignBaseline="@+id/textSymbol"
        android:layout_alignBottom="@+id/textSymbol"
        android:layout_toEndOf="@+id/textSymbol"
        android:layout_toRightOf="@+id/textSymbol"
        android:background="#FFF"
        android:textColor="#000"
        android:text="TextView" />

    <TextView
        android:id="@+id/textChange"
        android:layout_width="100dp"
        android:layout_height="33dp"
        android:layout_alignBaseline="@+id/textPrice"
        android:layout_alignBottom="@+id/textPrice"
        android:layout_toEndOf="@+id/textPrice"
        android:layout_toRightOf="@+id/textPrice"
        android:background="#FFF"
        android:textColor="#000"
        android:text="TextView" />

    <TextView
        android:id="@+id/textPercent"
        android:layout_width="80dp"
        android:layout_height="33dp"
        android:layout_alignBaseline="@+id/textChange"
        android:layout_alignBottom="@+id/textChange"
        android:layout_toEndOf="@+id/textChange"
        android:layout_toRightOf="@+id/textChange"
        android:background="#FFF"
        android:textColor="#000"
        android:text="TextView" />



</RelativeLayout>